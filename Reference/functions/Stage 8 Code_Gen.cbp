<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Functions" />
		<Option makefile_is_custom="1" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="all">
				<Option output="compiler" prefix_auto="1" extension_auto="1" />
				<Option object_output="." />
				<Option type="0" />
				<Option compiler="gcc" />
				<MakeCommands>
					<Build command="$make -f $makefile $target" />
					<CompileFile command="$make -f $makefile $file" />
					<Clean command="$make -f $makefile clean" />
					<DistClean command="$make -f $makefile distclean$target" />
					<AskRebuildNeeded command="$make -q -f $makefile $target" />
					<SilentBuild command="$make -f $makefile $target &gt; $(CMD_NULL)" />
				</MakeCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
		<Unit filename="compiler.l">
			<Option compile="1" />
		</Unit>
		<Unit filename="compiler.y">
			<Option compile="1" />
			<Option weight="0" />
		</Unit>
		<Unit filename="symbolTable.cpp" />
		<Unit filename="symbolTable.h" />
		<Unit filename="syntaxTree.cpp" />
		<Unit filename="syntaxTree.h" />
		<Unit filename="util.cpp">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="util.h" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
